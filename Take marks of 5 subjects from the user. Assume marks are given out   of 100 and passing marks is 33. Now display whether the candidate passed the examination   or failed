/* Write a program to take marks of 5 subjects from the user. Assume marks are given out
  of 100 and passing marks is 33. Now display whether the candidate passed the examination
  or failed. */

  #include<stdio.h>
  #include<conio.h>  
  
    void main()
  {
        int Eng,Hin,maths,chem,phy;
        float res;
        printf("Enter marks of 5 best subjects\n");
        scanf("%d%d%d%d%d",&Eng,&Hin,&maths,&chem,&phy);
        res = ( Eng + Hin + maths + chem + phy )*100/500;

        printf( (res >= 33)? "You are passed" : "You are failed");
    getch();
  }
