/* Write a program to find the greatest among three given numbers. Print number once 
if the greatest number appears two or three times.*/

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x,y,z;
        printf("Enter 3 number ");
        scanf("%d%d%d",&x,&y,&z);

        if (x>=y && x>=z )
            printf("%d",x);

        else if ( y>=z )
            printf("%d",y);

        else
            printf("%d",z);
        
    getch();
}
